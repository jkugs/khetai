  AfterClass: false
  AfterControlStatement: false
  AfterEnum: false
  AfterFunction: false
  AfterNamespace: false
  AfterStruct: false
  AfterUnion: false
  BeforeCatch: false
  BeforeElse: false
  IndentBraces: false
ColumnLimit: 0
AlignConsecutiveMacros: true
IndentCaseLabels: true
AllowShortCaseLabelsOnASingleLine: true